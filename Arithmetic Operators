# Variables
a = 15
b = 4

# Addition
print("Addition:", a + b)  

# Subtraction
print("Subtraction:", a - b) 

# Multiplication
print("Multiplication:", a * b)  

# Division
print("Division:", a / b) 

# Modulus
print("Modulus:", a % b) 

#OUTPUT
Addition: 19
Subtraction: 11
Multiplication: 60
Division: 3.75
Modulus: 3

